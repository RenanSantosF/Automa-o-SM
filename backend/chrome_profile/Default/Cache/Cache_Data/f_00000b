<script type="text/javascript">
function close_info(){
 $( "#doc-info" ).toggle();
  var x = document.getElementById("close-info");
  if (x.innerHTML === "i") {
    x.innerHTML = "X";
  } else {
    x.innerHTML = "i";
  }

  }
</script>
